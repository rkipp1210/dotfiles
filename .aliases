# List all files colorized in long format
alias ll='ls -lh'

# List all files colorized in long format, including dot files
alias la="ls -lha"

# List only directories
alias lsd='ls -l | grep "^d"'

# git status shorten
alias gits='git status'

# git commit shorten
alias gitc='git commit -m'

# git diff shorten
alias gitd='git diff'
